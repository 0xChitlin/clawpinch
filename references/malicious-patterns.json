{
  "meta": {
    "version": "1.0.0",
    "last_updated": "2026-02-06",
    "description": "Known malicious patterns from the ClawHavoc campaign and related supply chain attacks targeting the OpenClaw ecosystem."
  },
  "known_malicious_packages": [
    "clawhub-cli",
    "clawdhub",
    "openclaw-helper",
    "openclaw-utils",
    "phantom-wallet-skill",
    "solana-pay-pro",
    "claw-hub-cli",
    "openclaw-plugin-helper",
    "openclaw-core-utils",
    "clawhub-sdk",
    "openclaw-wallet-connect",
    "claw-skill-loader"
  ],
  "suspicious_domains": [
    "raw.githubusercontent.com/*/exec",
    "pastebin.com",
    "transfer.sh",
    "paste.ee",
    "hastebin.com",
    "cdn-claw.xyz",
    "openclaw-cdn.com",
    "clawstatic.net",
    "rentry.co",
    "gist.githubusercontent.com/*/raw"
